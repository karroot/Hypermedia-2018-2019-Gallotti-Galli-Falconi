{
    "extends": "eslint:recommended",
    "rules": {
	      "no-console": 0
	  },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "env": {
        "jquery": true,
        "node": true,
        "es6": true
    },
    "globals": {
        "require": true,
        "$": true
    }
}
